<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <connectionStrings>
    <!-- 
    Editï¿½ el valor de 'Server=' con el nombre de tu PC o 'localhost\SQLEXPRESS'.
    Ejemplos:
      Server=localhost\SQLEXPRESS
      Server=DESKTOP-TUNOMBRE\SQLEXPRESS
